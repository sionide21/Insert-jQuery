{
  "manifest_version": 2,
  "name" : "Insert jQuery",
  "version" : "0.1",

  "description" : "Quickly add jQuery to any page from the context menu.",
  "background" : {
    "scripts": ["insert-jquery.js"]
  },
  "web_accessible_resources": [
    "snippets/appendScript.js"
  ],
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
