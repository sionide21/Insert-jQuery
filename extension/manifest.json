{
  "manifest_version": 2,
  "name" : "Insert jQuery",
  "version" : "1.0.0",

  "description" : "Quickly add jQuery to any page from the context menu.",
  "icons": {
    "16": "icons/logo16.png",
    "48": "icons/logo48.png",
    "128": "icons/logo128.png"
  },
  "background" : {
    "scripts": ["insert-jquery.js"]
  },
  "web_accessible_resources": [
    "snippets/appendScript.js"
  ],
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}
